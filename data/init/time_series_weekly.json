{
  "settings": {
    "number_of_shards": 3,
    "number_of_replicas": 1
  },
  "mappings": {
    "dynamic": "strict",
    "properties": {
      "symbol": {
        "type": "keyword"
      },
      "timestamp": {
        "type": "date",
        "format": "yyyy-MM-dd"
      },
      "open": {
        "type": "float"
      },
      "high": {
        "type": "float"
      },
      "low": {
        "type": "float"
      },
      "close": {
        "type": "float"
      },
      "volume": {
        "type": "integer"
      }
    }
  }
}

